{
    "name": "patcon/drupalcs",
    "description": "Drupal Standards for PHP_CodeSniffer",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "squizlabs/PHP_CodeSniffer",
                "version": "1.3.3-dev",
                "source": {
                    "url": "git://github.com/squizlabs/PHP_CodeSniffer.git",
                    "type": "git",
                    "reference": "9ce8010f30b44e"
                },
                "autoload": {
                    "classmap": [ "." ]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/php-timer",
                "version": "1.0.2",
                "source": {
                    "url": "git://github.com/sebastianbergmann/php-timer.git",
                    "type": "git",
                    "reference": "1.0.2"
                },
                "autoload": {
            	    "classmap": [ "PHP/" ]
                }
            }
        }
    ],
    "require": {
        "squizlabs/PHP_CodeSniffer": "*",
        "sebastianbergmann/php-timer": "1.0.*"
    }
}

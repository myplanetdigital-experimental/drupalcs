{
    "name": "patcon/drupalcs",
    "description": "Drupal Standards for PHP_CodeSniffer",
    "repositories": {
        "phpcs-github": {
            "type": "package",
            "package": {
                "name": "squizlabs/PHP_CodeSniffer",
                "version": "1.3.3-dev",
                "source": {
                    "url": "git://github.com/squizlabs/PHP_CodeSniffer.git",
                    "type": "git",
                    "reference": "9ce8010f30b44e"
                }
            }
        },
        "phpunit": {
            "type": "pear",
            "url": "http://pear.phpunit.de"
        }
    },
    "require-dev": {
        "squizlabs/PHP_CodeSniffer": ">=1.3",
        "pear-phpunit/PHP_Timer": "*"
    },
    "autoload": {
        "classmap": [ "." ]
    }
}
